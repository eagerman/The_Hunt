=== Modified Makefile
LDLIBS = -lSegFault -rdynamic
# Makefile for "Fury of Dracula": The Hunt

# change these to suit your local C environment
CC = gcc # -std=c99
CFLAGS = -gdwarf-2 #-Wall 
# do not change the following line
BINS = dracula hunter
# add any other *.o files that your system requires
# (and add their dependencies below after DracView.o)
# if you're not using Map.o or Places.o, you can remove them
OBJS = GameView.o Map.o Places.o
# add whatever system libraries you need here (e.g. -lm)
LIBS = -ljansson 

all : $(BINS)

dracula : dracPlayer.o dracula.o DracView.o $(OBJS) $(LIBS) -ljansson 
hunter : hunterPlayer.o hunter.o HunterView.o Queue.o $(OBJS) $(LIBS) -ljansson 
 

dracPlayer.o : player.c Game.h DracView.h dracula.h
	$(CC)  -gdwarf-2  -DI_AM_DRACULA -c player.c -o dracPlayer.o

hunterPlayer.o : player.c Game.h HunterView.h hunter.h
	$(CC)  -gdwarf-2  -c player.c -o hunterPlayer.o

dracula.o : dracula.c Game.h DracView.h
hunter.o : hunter.c Game.h HunterView.h
Places.o : Places.c Places.h
Map.o : Map.c Map.h Places.h
GameView.o : GameView.c Globals.h GameView.h
HunterView.o : HunterView.c Globals.h HunterView.h
DracView.o : DracView.c Globals.h DracView.h
Queue.o : Queue.c Queue.h

# if you use other ADTs, add dependencies for them here

clean :
	rm -f $(BINS) *.o core


=== Compiling Dracula ...
gcc  -gdwarf-2    -c -o dracula.o dracula.c
gcc   -gdwarf-2  -DI_AM_DRACULA -c player.c -o dracPlayer.o
gcc  -gdwarf-2    -c -o DracView.o DracView.c
gcc  -gdwarf-2    -c -o GameView.o GameView.c
gcc  -gdwarf-2    -c -o Map.o Map.c
gcc  -gdwarf-2    -c -o Places.o Places.c
gcc    dracula.o dracPlayer.o DracView.o GameView.o Map.o Places.o /usr/lib/i386-linux-gnu/libjansson.so  -lSegFault -rdynamic -o dracula
Dracula AI compiled OK
=== Compiling Hunter ...
gcc  -gdwarf-2    -c -o hunter.o hunter.c
gcc   -gdwarf-2  -c player.c -o hunterPlayer.o
gcc  -gdwarf-2    -c -o HunterView.o HunterView.c
gcc  -gdwarf-2    -c -o Queue.o Queue.c
gcc    hunter.o hunterPlayer.o HunterView.o Queue.o GameView.o Map.o Places.o /usr/lib/i386-linux-gnu/libjansson.so  -lSegFault -rdynamic -o hunter
Hunter AI compiled OK

